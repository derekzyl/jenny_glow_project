{
  "name": "@jenny-glow/backend",
  "version": "3.0.9",
  "description": "JennyGlow  app",
  "main": "dist/index.js",
  "bin": "bin/createNodetsApp.cjs",
  "repository": "https://git@github.com:PAYPADDY/backend-core.git",
  "author": "example author <example@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16 <23"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:build": "yarn compile && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc && tsc-alias",
    "compile:watch": "tsc && concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "pre:dev": "concurrently --kill-others \"yarn pre:dev:worker\" \"yarn pre:dev:main\"",
    "pre:dev:main": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "pre:dev:worker": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/modules/worker/manager.jobs.worker.js",
    "dev": "concurrently --kill-others \"yarn compile\" \"yarn pre:dev\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles",
    "test:ts": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'ts$'",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'js$'",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --watchAll",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage",
    "coverage:coveralls": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage --coverageReporters=lcov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@jest/globals": "^27.5.1",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "2.8.8",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.17.0",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.11.12",
    "@types/node-forge": "^1.3.5",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/sha.js": "^2.4.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.6",
    "@types/validator": "^13.7.0",
    "@types/ws": "^8.5.6",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.8.7",
    "typedoc": "^0.25.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@hokify/agenda": "^6.3.0",
    "@socket.io/cluster-adapter": "^0.2.2",
    "@socket.io/sticky": "^1.0.4",
    "aws-sdk": "^2.1488.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "big-integer": "^1.6.52",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.1",
    "expressbolt": "^1.0.17",
    "helmet": "^4.6.0",
    "http-status": "^1.7.4",
    "joi": "^17.11.1",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "sha.js": "^2.4.11",
    "sharp": "^0.33.3",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "validator": "^13.7.0",
    "winston": "^3.12.0",
    "xss-clean": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
